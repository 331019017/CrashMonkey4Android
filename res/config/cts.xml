<configuration description="Runs a CTS plan from a pre-existing CTS installation" >

    <option
        name="wifiSsdk"
        value="58-guest" />

    <option
        name="wifiPsk"
        value="58shenqi" />

    <option
        name="p"
        value="com.wuba" />

    <option
        name="a"
        value="com.wuba.activity.launch.LaunchActivity" />

    <option
        name="v"
        value="20" />

    <option
        name="app-path"
        value="/Users/wuxian/Downloads/58house_debug_58WuxianClient_online_v6.1.0.0_3.apk" />

    <option
        name="throttle"
        value="300" />

    <option
        name="skip-device-info"
        value="false" />

    <option
        name="reboot"
        value="true" />

    <option
        name="enable-root"
        value="false" />

    <option
        name="skip-uninstall-app"
        desc="测试结束后，一般会卸载测试应用，可以通过该选项设置是否卸载，true为不卸载"
        value="true" />

    <option
        name="skip-device-unlock"
        desc="是否跳过设备信息获取的步骤,true为不获取设备信息"
        value="false" />

    <option
        name="pct-tap"
        desc="点击事件的百分比"
        value="25" />

    <option
        name="pct-motion"
        desc="点击事件的百分比"
        value="0" />

    <option
        name="pct-drag"
        desc="点击事件的百分比"
        value="30" />

    <option
        name="pct-nav"
        desc="点击事件的百分比"
        value="15" />

    <option
        name="pct-majornav"
        desc="点击事件的百分比"
        value="15" />

    <option
        name="pct-syskeys"
        desc="点击事件的百分比"
        value="15" />

    <build_provider class="com.android.cts.tradefed.build.CtsBuildProvider" />

    <device_recovery class="com.android.tradefed.device.WaitDeviceRecovery" />

    <test class="com.android.cts.tradefed.testtype.MonkeyTest" />

    <logger class="com.android.tradefed.log.StdoutLogger" />

    <result_reporter class="com.android.cts.tradefed.result.CtsXmlResultReporter" />

    <result_reporter class="com.android.cts.tradefed.result.IssueReporter" />

</configuration>